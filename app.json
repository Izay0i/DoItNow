{
  "expo": {
    "name": "DoItNow",
    "slug": "DoItNow",
    "version": "1.0.0",
    "scheme": "exp",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffa500"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.doitnow",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffa500"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/images/logo.png"
    },
    "notification": {
      "icon": "./assets/images/logo.png",
      "color": "#ffa500"
    },
    "plugins": [
      [
        "expo-notifications", {
          "icon": "./assets/images/logo.png",
          "color": "#ffa500"
        }
      ]
    ]
  }
}